# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Golos' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for golos-ios

  # IQKeyboardManager
  pod 'IQKeyboardManagerSwift'

  # SwiftLint
  pod 'SwiftLint'

  # HockeyApp
  pod 'HockeySDK', '~> 5.1.2'

  # Starscream
  pod 'Starscream', '~> 3.0.2'

  # PINCache
  pod 'PINCache'

  # Markdown / HTML
  pod "MarkdownView"
  pod "Down"
  pod 'MarkedView'

end
